public class no1 {
    public static void main (String[] args){
      int a = 7;
        int hsl = 0 ;
        int[] arr = new int[a];
        arr[0] =1;
        arr[1] =1;        
        for (int i = 2 ; i < a ; i++) {
            arr[i]= arr[i-1] + arr[i-2];
        }
            for (int i = 0 ; i < a ; i++) {
                hsl=hsl+arr[i];
            }
            System.out.println(hsl);
       }
}
